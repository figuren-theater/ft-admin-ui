{
    "name": "figuren-theater/ft-admin-ui",
    "description": "Clean and helpful UI to make your digital content editing with websites.fuer.figuren.theater a friendly experience.",
    "type": "wordpress-muplugin",
    "license": "GPL-3",
    "version": "1.0.0",
    "extra": {
        "installer-name": "Admin_UI"
    },
    "autoload": {
        "psr-4": {
            "Figuren_Theater\\Admin_UI\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Carsten Bach"
        }
    ],
    "prefer-stable": true,
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/figuren-theater/ft-core"
        },
        {
            "type": "composer",
            "url" : "https://wpackagist.org"
        }
    ],
    "require": {
        "php": ">=7.1",
        "figuren-theater/ft-core": "^1",
        "composer/installers": "~1",
        "wpackagist-plugin/disable-gutenberg-blocks": "1.0.9",
        "wpackagist-plugin/heartbeat-control": "^2.0",
        "wpackagist-plugin/multisite-enhancements": "1.6.1",
    }
}
